{
  "name": "netstreams",
  "author": "Alvaro Fernandez nishinishi9999@protonmail.com",
  "version": "0.1.0",
  "description": "Simple terminal net stream player",
  "main": "src/js/main.js",
  "scripts": {
    "start": "NODE_PATH=./src/js node src/js/main.js",
    "test": "mocha test/test.js || true",
    "build": "tsc || true",
    "pkg": "pkg src/js/main.js -t node10-linux-x64 -o bin/main && cp src/ts/config.json bin",
    "doc": "jsdoc -r -c jsdoc.json -d doc || true"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "blessed": "^0.1.81",
    "jsdom": "^14.0.0",
    "jsonfile": "^4.0.0",
    "minimist": "^1.2.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.10",
    "@types/jsdom": "^12.2.3",
    "@types/jsonfile": "^4.0.1",
    "@types/minimist": "^1.2.0",
    "@types/request": "^2.47.0"
  }
}
