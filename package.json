{
  "name": "termice",
  "author": "Alvaro Fernandez nishinishi9999@protonmail.com",
  "version": "1.0.0",
  "description": "Simple terminal icecast player",
  "main": "src/js/main.js",
  "scripts": {
    "start": "TERMICE_ENV=test node src/js/main.js",
    "build": "tsc -p .",
    "pkg64": "pkg src/js/main.js -t node10-linux-x64 -o bin/termice64 && cp src/conf bin -r",
    "pkg86": "pkg src/js/main.js -t node10-linux-x86 -o bin/termice86 && cp src/conf bin -r",
    "lint": "tslint src/ts/**/**"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "blessed": "^0.1.81",
    "jsdom": "^14.0.0",
    "jsonfile": "^4.0.0",
    "minimist": "^1.2.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.10",
    "@types/jsdom": "^12.2.3",
    "@types/jsonfile": "^4.0.1",
    "@types/minimist": "^1.2.0",
    "@types/request": "^2.47.0"
  }
}
